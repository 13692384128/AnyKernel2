<!--Ids are used in identifying the boosts, increment ids linearly,
    do not modify Ids, or add in between-->
<!--follow the number format rules while specifying number, can be given
    in any format-->

<BoostConfigs>
    <PerfBoost>

    <!--app lauch boost-->

        <!-- Type="1", main launch boost of 2sec -->
        <Config
            Id="0x00001081" Type="1" Enable="false" Timeout="2000" Target="msm8998"
            Resources="" />

    <!--app subseq launch boost-->

        <Config
            Id="0x00001082" Type="1" Enable="false" Timeout="300" Target="msm8998"
            Resources="" />

    <!--app lauch boost (disabling packing)-->

        <!-- Type="2", launch boost for disable packing 1.5sec -->
        <Config
            Id="0x00001081" Type="2" Enable="false" Timeout="1500" Target="msm8998"
            Resources="" />

    <!-- config_enableLaunchBoostv2-->

        <!-- Type="3", FA launch boost of 15sec -->
        <Config
            Id="0x00001081" Type="3" Enable="false" Timeout="15000" Target="msm8998"
            Resources="" />

    <!-- config_enableLaunchBoostv4-->

        <!-- Type="4", Adaptive Launch boost -->
        <Config
            Id="0x00001081" Type="4" Enable="false" Timeout="2000" Target="msm8998"
            Resources="" />

    <!--Vertical Scroll boost-->
        
        <!-- Type="1", Vertical Scroll boost -->
        <Config
            Id="0x00001080" Type="1" Enable="false" Target="msm8998"
            Resources="" />

    <!--Horizontal Scroll boost-->

        <!-- Type="2", Horizontal Scroll boost -->
        <Config
            Id="0x00001080" Type="2" Enable="false" Target="msm8998"
            Resources="" />

    <!--Pre-Fling boost-->

        <!-- Type="4", Pre-Fling boost -->
        <Config
            Id="0x00001080" Type="4" Enable="false" Timeout="120" Target="msm8998"
            Resources="" />

    <!--PackageInstall boost-->

        <!-- Type="", Package Install boost -->
        <Config
            Id="0x00001088" Enable="false" Target="msm8998"
            Resources="" />

    <!--Rotation latency boost-->

        <!-- Type="", Rotation latency boost -->
        <Config
            Id="0x00001089" Enable="false" Timeout="1500" Target="msm8998"
            Resources="" />

    <!--Rotation animation boost-->

        <!-- Type="", Rotation animation boost -->
        <Config
            Id="0x00001090" Enable="false" Timeout="1000" Target="msm8998"
            Resources="" />
    </PerfBoost>
</BoostConfigs>
